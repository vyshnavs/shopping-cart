<section>
  <div class="container">
    <div class="row">
      <a href="/admin/add-trending" class="btn btn-success ml-auto mt-4">Delete user account</a>
    </div>
    <a href="/admin" class="btn btn-primary">ADMIN PANEL</a>
    <table class="table mt-5">
      <thead>
        <tr>
          <th scope="col">No</th>
          <th scope="col">Name</th>
          <th scope="col">Email</th>
        </tr>
      </thead>
      <tbody>
        {{#each users}}

        <tr>
          <th scope="row">1</th>
          <td>{{this.name}}</td>
          <td>{{this.email}}</td>
        </tr>
        {{/each}}

      </tbody>
    </table>
  </div>
</section>